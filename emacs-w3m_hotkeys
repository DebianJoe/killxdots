#!/bin/bash
echo "SPC, C-v Forward Page"
echo "b, ESC-v Back Page"
echo "j, cursor down"
echo "k, cursor up"
echo "TAB, next hyperlink"
echo "RET follow link"
echo "u, peek link url"
echo "U, open url"
echo "V, open new file"
echo "# exec shell command and browse"
echo "s, select buffer"
echo "esc b, load bookmark"
echo "esc a, add bookmark"
echo "/ search forward"
echo "? search backwards"
echo "H, load help"
echo "C^Space set mark"
echo "ESC^p goto previous mark"
echo "ESC^n goto next mark"
